<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 触摸模式下单击时的背景图片 -->

    <item android:drawable="@drawable/edittext_bg_n" android:state_focused="false" android:state_pressed="true"/>

    <!-- 选中时的图片背景 -->

    <item android:drawable="@drawable/edittext_bg_s" android:state_selected="true"/>

    <!-- 获得焦点时的图片背景 -->

    <item android:drawable="@drawable/edittext_bg_s" android:state_focused="true"/>
    <!-- 没有焦点时的背景图片 -->
    <item android:drawable="@drawable/edittext_bg_s" android:state_window_focused="false"/>

    <!-- 默认时的背景图片 -->
    <item android:drawable="@drawable/edittext_bg_s"/>
    
    
    
